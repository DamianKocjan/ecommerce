{
	"name": "@ecommerce/db",
	"version": "0.1.0",
	"main": "./index.ts",
	"types": "./index.ts",
	"license": "MIT",
	"scripts": {
		"clean": "rm -rf .turbo node_modules",
		"db:generate": "pnpm with-env prisma generate",
		"db:push": "pnpm with-env prisma db push --skip-generate",
		"db:seed": "pnpm with-env ts-node seed/index.ts",
		"dev": "pnpm with-env prisma studio --port 5556",
		"with-env": "dotenv -e ../../.env --",
		"lint": "eslint",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@prisma/client": "^5.0.0"
	},
	"devDependencies": {
		"@types/node": "^20.4.5",
		"dotenv-cli": "^7.2.1",
		"prisma": "^5.0.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6"
	}
}
